{
    "keyboard_name": "Keychron V1 Max",
    "manufacturer": "Keychron",
    "url": "https://github.com/Keychron",
    "maintainer": "lokher",
    "processor": "STM32F401",
    "bootloader": "stm32-dfu",
    "usb": {
        "vid": "0x3434"
    },
    "features": {
        "bootmagic": true,
        "extrakey" : true,
        "mousekey" : true,
        "dip_switch" : true,
        "nkro" : true,
        "rgb_matrix": true,
        "raw" : true,
        "send_string" : true
    },
    "matrix_pins": {
        "cols": ["C6", "C7", "C8", "A14", "A15", "C10", "C11", "C13", "C14", "C15", "C0", "C1", "C2", "C3", "A0", "A1"],
        "rows": ["C12", "D2", "B3", "B4", "B5", "B6"]
    },
    "diode_direction": "ROW2COL",
    "dip_switch" :{
        "pins": ["A3"]
    },
    "rgb_matrix": {
        "driver": "snled27351_spi",
        "sleep": true,
        "animations": {
            "band_spiral_val": false,
            "breathing": false,
            "cycle_all": false,
            "cycle_left_right": false,
            "cycle_out_in": false,
            "cycle_out_in_dual": false,
            "cycle_pinwheel": false,
            "cycle_spiral": false,
            "cycle_up_down": false,
            "digital_rain": false,
            "dual_beacon": false,
            "jellybean_raindrops": false,
            "pixel_rain": false,
            "rainbow_beacon": false,
            "rainbow_moving_chevron": false,
            "solid_reactive_multinexus": false,
            "solid_reactive_multiwide": false,
            "solid_reactive_simple": false,
            "solid_splash": false,
            "splash": false,
            "typing_heatmap": false
        }
    },
    "eeprom": {
        "wear_leveling": {
            "driver": "embedded_flash",
            "logical_size": 2048,
            "backing_size": 4096
        }
    },
    "build": {
        "debounce_type": "sym_eager_pk"
    },
    "debounce": 20
}
